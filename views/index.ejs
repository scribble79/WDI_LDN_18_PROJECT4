<!DOCTYPE html>
<html ng-app="ChildrensCenters">
<head>
  <title>Children's Centre's</title>

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBxM36DxD-TmmCuWMML0UXBVSirOUkF42Q"></script>
  <script src="/vendor/angular/angular.min.js"></script>
  <script src="/vendor/angular-resource/angular-resource.min.js"></script>
  <script src="/vendor/angular-jwt/dist/angular-jwt.min.js"></script>
  <script src="/vendor/angular-ui-router/release/angular-ui-router.min.js"></script>
  <script src="/vendor/angular-bootstrap/ui-bootstrap.min.js"></script>
  <script src="/vendor/angular-animate/angular-animate.min.js"></script>
  <script src="/vendor/angular-ticker/release/ticker.js"></script>

  <script src="/js/app.js"></script>
  <script src="/js/controllers/navBarController.js"></script>
  <script src="/js/controllers/tickerController.js"></script>
  <script src="/js/controllers/adminsController.js"></script>
  <script src="/js/models/admin.js"></script>
  <script src="/js/controllers/centersController.js"></script>
  <script src="/js/models/center.js"></script>
  <script src="/js/controllers/eventsController.js"></script>
  <script src="/js/models/event.js"></script>
  <script src="/js/controllers/mapController.js"></script>
  <script src="/js/services/token.js"></script>
  <script src="/js/services/authInterceptor.js"></script>

  <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<!-- 
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" integrity="sha512-dTfge/zgoMYpP7QbHy4gWMEGsbsdZeCXz7irItjcC3sPUFtf0kuFbDz/ixG7ArTxmDjLXDmezHubeNikyKGVyQ==" crossorigin="anonymous">
-->
<link rel="stylesheet" type="text/css" href="css/custom.css">

<link rel="stylesheet" type="text/css" href="/css/style.css">

<link rel="stylesheet" href="/vendor/angular-ticker/release/ticker.css">

</head>
<body ng-controller="adminsController as admins">
  <nav ng-controller="navBarController as nav" class="navbar navbar-default navbar-fixed-top">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" ui-sref="home">
          <img alt="Brand" src="/images/Southwarklogo.png" width="90px">
        </a>
        <ul class="nav navbar-nav navbar-right">
          <span class="dropdown" uib-dropdown on-toggle="toggled(open)">
            <a href id="simple-dropdown" uib-dropdown-toggle>
              Children's Centres
            </a>
            <ul uib-dropdown-menu aria-labelledby="simple-dropdown">
            <!-- <li ng-repeat="item in nav.items">
                <a href>{{item.name}}</a> -->
                <li><a ui-sref="home">Home</a></li>
                <li><a ui-sref="info">Information For Families</a></li>
                <li><a ui-sref="center">Kintore Way</a></li>
                <li><a ui-sref="center">Pilgrims Way</a></li>
                <li><a ui-sref="center">South Bermondsey</a></li>
                <li><a ui-sref="center">Rotherhite</a></li>
              </li>
            </ul>
          </span>
        </ul> 
      </div>
    </div>
  </nav>


  <main ui-view></main>


  <ul ng-controller="tickerController as tickerCtl" ticker timing="2500">
    <li ng-repeat="item in tickerCtl.myTickerItems">
      {{item.title}} - {{item.copy}}
    </li>
  </ul>

</body>
</html>